    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'azure'",
            "cargo": {
                "args": [
                    "build",
                    // "--package",
                    // "azure-cli",
                    "--bin",
                    "azure",
                    "--features=runtime-tokio",
                ],
                "filter": {
                    "name": "azure",
                    "kind": "bin"
                }
            },
            "args": [
                "api",
                "network",
                "vnet",
                "show",
                "--resource-group",
                "qinkaiwu-test-rg",
                "--subscription",
                "fake",
                "--name",
                "qinkaiwu-test-vnet"
            ],
            "cwd": "${workspaceFolder}"
        }
    ]
}